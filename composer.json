{
  "name": "donnysim/validation",
  "description": "Data validator for PHP.",
  "keywords": [
    "php",
    "validator"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "donnysim"
    }
  ],
  "require": {
    "php": "^8.1",
    "ext-bcmath": "*",
    "ext-json": "*",
    "brick/math": "^0.10.2",
    "egulias/email-validator": "^3.2",
    "nesbot/carbon": "^2.62"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "DonnySim\\Validation\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "DonnySim\\Validation\\Tests\\": "tests"
    }
  }
}
