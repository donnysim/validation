{
  "name": "donnysim/validation",
  "description": "Data validator for PHP.",
  "keywords": [
    "validator",
    "laravel"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "donnysim"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-bcmath": "*",
    "ext-json": "*",
    "brick/math": "^0.9.1",
    "egulias/email-validator": "^2.1",
    "illuminate/database": "^v8.0",
    "illuminate/support": "^8.0",
    "illuminate/validation": "^8.0",
    "nesbot/carbon": "^2.42"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "DonnySim\\Validation\\": "src"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "DonnySim\\Validation\\Tests\\": "tests"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "DonnySim\\Validation\\Laravel\\ServiceProvider"
      ]
    }
  }
}
