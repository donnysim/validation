{
  "name": "donnysim/validation",
  "description": "Data validator for PHP.",
  "keywords": [
    "validator"
  ],
  "type": "library",
  "license": "MIT",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "donnysim"
    }
  ],
  "require": {
    "php": ">=7.4",
    "illuminate/support": "^6 || ^7 || ^8",
    "nesbot/carbon": "^2.42",
    "egulias/email-validator": "^2.1"
  },
  "autoload": {
    "psr-4": {
      "DonnySim\\Validation\\": "src"
    },
    "files": [
      "helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "DonnySim\\Validation\\Tests\\": "tests"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
